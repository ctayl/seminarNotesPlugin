{
  "name": "seminarNotesPlugin",
  "version": "1.0.0",
  "description": "This Buildfire plugin will show seminar notes.",
  "main": "index.js",
  "scripts": {
    "postinstall": "./node_modules/.bin/bower install",
    "test": "./node_modules/.bin/karma start --single-run --browsers PhantomJS",
    "build": "gulp build && cd .. && rm -rf seminarNotesPlugin.zip && zip -r seminarNotesPlugin_release.zip seminarNotesPlugin_release/ && rm -rf seminarNotesPlugin_release/",
    "build-debug": "cd .. && rm -rf seminarNotesPlugin_debug.zip && zip -r seminarNotesPlugin_debug.zip seminarNotesPlugin/ -x '*node_modules/*'"
  },
  "license": "ISC",
  "dependencies": {
    "bower": "^1.5.3",
    "karma-cli": "^0.1.0",
    "karma-coverage": "^0.5.0"
  },
  "devDependencies": {
    "chai": "^3.0.0",
    "del": "^4.0.0",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-csso": "^3.0.1",
    "gulp-eslint": "^5.0.0",
    "gulp-html-replace": "^1.6.2",
    "gulp-htmlmin": "^5.0.1",
    "gulp-minify-inline": "^1.1.0",
    "gulp-sequence": "^1.0.0",
    "gulp-strip-comments": "^2.5.2",
    "gulp-strip-debug": "^3.0.0",
    "gulp-uglify": "^3.0.2",
    "gulp-util": "^3.0.8",
    "jasmine-core": "^2.3.4",
    "karma": "^0.13.9",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.0",
    "karma-coverage": "^0.5.1",
    "karma-jasmine": "^0.3.6",
    "karma-junit-reporter": "^0.3.3",
    "karma-mocha": "^0.2.0",
    "karma-phantomjs-launcher": "^0.2.1",
    "mocha": "^2.2.5",
    "phantomjs": "^1.9.17"
  }
}
